# Environment Variables Example

# Flask application settings
FLASK_APP=flask_ui.app
FLASK_ENV=development
SECRET_KEY=your_secret_key_here

# MQTT settings
MQTT_BROKER_URL=your_mqtt_broker_url_here
MQTT_BROKER_PORT=1883
MQTT_USERNAME=your_mqtt_username_here
MQTT_PASSWORD=your_mqtt_password_here

# Telegram bot settings
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Home Assistant settings
HOME_ASSISTANT_URL=http://your_home_assistant_url_here
HOME_ASSISTANT_TOKEN=your_home_assistant_token_here

# VPN settings
VPN_CONFIG_PATH=/path/to/your/vpn/config

# DuckDNS settings
DUCKDNS_TOKEN=your_duckdns_token_here
DUCKDNS_DOMAIN=your_duckdns_domain_here

# Other settings
REVERSE_SSH_USER=your_reverse_ssh_user_here
REVERSE_SSH_HOST=your_reverse_ssh_host_here
REVERSE_SSH_PORT=22